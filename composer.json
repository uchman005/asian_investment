{
    "name": "golojan/golojan",
    "description": "GOLOJAN TECHNOLOGIES LTD",
    "type": "project",
    "version": "20.9.30",
    "keywords": [
        "psr4",
        "template"
    ],
    "license": "BSD",
    "authors": [
        {
            "name": "Golojan",
            "email": "info@golojan.com",
            "homepage": "https://golojan.com/",
            "role": "Developer"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "classmap": [
            "_apps/",
            "_apps/Apps/"
        ],
        "psr-4": {
            "Apps\\": [
                "_apps/",
                "_apps/Apps/"
            ]
        }
    },
    "require": {
        "php": ">=5.6.36"
    },
    "scripts": {},
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "application"
    ]
}